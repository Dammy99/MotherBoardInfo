<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:MotherBoardInfo.Views"
                    xmlns:vm="clr-namespace:MotherBoardInfo.ViewModels">
    <DataTemplate DataType="{x:Type vm:CPUViewModel}">
        <view:CPUView/>
    </DataTemplate>


    <DataTemplate DataType="{x:Type vm:GPUViewModel}">
        <view:GraphicView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:MemoryViewModel}">
        <view:MemoryView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:MotherboardViewModel}">
        <view:MotherboardView/>
    </DataTemplate>
</ResourceDictionary>